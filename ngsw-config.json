{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "prefetch-assets",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/assets/splash-screen/*",
        "/assets/*.css"
      ]
    }
  }, {
    "name": "lazy-assets",
    "installMode": "lazy",
    "resources": {
      "files": [
        "/assets/img/**",
        "/assets/*.css"
      ]
    }
  },
  {
    "name": "static-assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "urls":  [  
        "https://fonts.googleapis.com/icon?family=Material+Icons",
        "https://fonts.googleapis.com/css?family=Roboto|Roboto+Mono"
      ],
      "files": [
        "/assets/documents/*.html",
        "/assets/js/*"
      ]
    }
  }]
}
